ARDUINO_HOME_VALUE=~/Programmering/Arduino/arduino-1.0/
ARDUINO_PORT_VALUE=/dev/ttyACM0
ARDUINO_VER_VALUE=100
ARDUINO_BOARD_VALUE=uno
EXTRA_LIB_VALUE=./libraries
SCONSTRUCT_VALUE=./SConstruct

scons --sconstruct=$SCONSTRUCT_VALUE \
		ARDUINO_HOME=$ARDUINO_HOME_VALUE \
		ARDUINO_PORT=$ARDUINO_PORT_VALUE \
		ARDUINO_VER=$ARDUINO_VER_VALUE \
		ARDUINO_BOARD=$ARDUINO_BOARD_VALUE \
		EXTRA_LIB=$EXTRA_LIB_VALUE \
		$1
